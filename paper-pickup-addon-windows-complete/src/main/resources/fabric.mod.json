{
  "schemaVersion": 1,
  "id": "paper-pickup-addon",
  "version": "1.0.0",
  "name": "Paper Pickup Addon",
  "description": "Meteor Client addon that automatically picks up paper items within a 6-block radius",
  "authors": [
    "PaperAddon"
  ],
  "contact": {
    "homepage": "https://github.com/example/paper-addon",
    "sources": "https://github.com/example/paper-addon"
  },
  "license": "GPL-3.0",
  "icon": "assets/paper-pickup-addon/icon.png",
